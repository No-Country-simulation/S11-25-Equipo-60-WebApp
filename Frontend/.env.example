# Env Example file for Frontend application
# Copia este archivo a .env.local y configura tus valores

# URL del Frontend
NEXT_FRONTEND_URL=your_frontend_url

# ID de la organización
NEXT_ID_ORG=your_organization_id

# URL del Backend API
NEXT_BACKEND_API_URL=your_backend_api_url

# API Key para autenticación con el backend
NEXT_BACKEND_API_KEY=your_backend_api_key

# Environment (development, production)
NODE_ENV=development